[configuration]
entry_symbol = "GDExtensionInit"
compatibility_minimum = 4.4
reloadable=true

[icons]
Example = "icons/Example.svg"

[libraries]
linux.release.x86_64 = "lib/Linux-x86_64/lib${PROJECT_NAME}.so"
macos.release = "lib/Darwin-universal/lib${PROJECT_NAME}.dylib"
windows.release.x86_64 = "lib/Windows-AMD64/${LIB_PREFIX}${PROJECT_NAME}.dll"
android.arm64 = "lib/Android-arm64-v8a/lib${PROJECT_NAME}.so"
android.arm32 = "lib/Android-armv7-a/lib${PROJECT_NAME}.so"

[dependencies]
windows.x86_64 = { 
    "res://lib/Windows-AMD64/legacy.dll": "",
    "res://lib/Windows-AMD64/libcrypto-3-x64.dll": "",
    "res://lib/Windows-AMD64/libssl-3-x64.dll": "",
    "res://lib/Windows-AMD64/lua51.dll": "",
    "res://lib/Windows-AMD64/luv.dll": "",
}